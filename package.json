{
  "name": "fox-game",
  "version": "1.0.0",
  "description": "Fun frontend project using HTML, CSS and JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "dev": "parcel src/index.html",
    "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,css}\"",
    "format:check": "prettier ./.gitignore --check \"./**/*.{html,json,js,ts,css}\"",
    "lint": "eslint --ignore-path ./.gitignore --fix \"./**/*.{js,ts}\"",
    "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cornelia-schulz/fox-game.git"
  },
  "keywords": [
    "fox",
    "frontend",
    "HTML",
    "CSS",
    "JavaScript",
    "Frontend",
    "Masters"
  ],
  "author": "Cornelia Schulz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cornelia-schulz/fox-game/issues"
  },
  "homepage": "https://github.com/cornelia-schulz/fox-game#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "browserslist": [
    "last 2 Firefox versions"
  ]
}
